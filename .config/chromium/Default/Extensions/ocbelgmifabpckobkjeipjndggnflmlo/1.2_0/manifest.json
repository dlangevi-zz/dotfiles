{
   "content_scripts": [ {
      "css": [ "modules/content/content.css" ],
      "js": [ "modules/common/underscore-min.js", "modules/common/jquery.min.js", "modules/content/PageEnvironment.js", "modules/content/PageEnvironment.js", "modules/content/content.js" ],
      "matches": [ "*://soundcloud.com/*" ]
   } ],
   "description": "Adds a button to SoundCloud for repeating the current song.",
   "icons": {
      "128": "icons/icon128.png",
      "19": "icons/icon19.png",
      "38": "icons/icon38.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "SoundCloud Repeat for Chrome",
   "permissions": [ "tabs", "*://soundcloud.com/*" ],
   "short_name": "SoundCloud Repeat",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.2",
   "web_accessible_resources": [ "modules/content/repeat.png", "modules/content/repeat_small.png" ]
}
